# curly-giggle
#Find the minimum ad maximum element in array
class Solution{
    getMinMax(arr,n){
        var min = arr[0];
        var max = arr[0];
        for(var i=1;i<n;i++){
            if(arr[i]<min){
                min = arr[i]
                
            }
            if(arr[i]>max){
                max = arr[i]
            }
        }
        return[min , max]; 
    }
}
